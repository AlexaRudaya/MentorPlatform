{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "Logs\\logs.json",
          "outputTemplate": "{Timestamp:G} {Message}{NewLine:1}{Exception:1}",
          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
        }
      }
    ]
  },
  "Kestrel": {
    "Endpoints": {
      "Api": {
        "Url": "https://mentor_api:7001",
        "Protocols": "Http1"
      },
      "Grpc": {
        "Url": "https://mentor_api:7002",
        "Protocols": "Http2"
      }
    }
  },
  "ConnectionStrings": {
    "MentorsConnection": "Server=mentor_db;Database=MentorDb;User Id=sa;Password=Ms!1password;MultiSubnetFailover=True;Encrypt=False;TrustServerCertificate=True"
  },
  "Authentication": {
    "Authority": "https://localhost:44374",
    "Audience": "516c8f68-2d32-44c0-8a24-1ecf303f3657"
  }
}